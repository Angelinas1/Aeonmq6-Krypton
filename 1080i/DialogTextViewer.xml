<?xml version="1.0" encoding="UTF-8"?>
<window id="147">
    <!-- Aeon MQ 6 -->
    <defaultcontrol>61</defaultcontrol>
    <include>Animation_Fade</include>
    <coordinates>
        <system>1</system>
        <left>0</left>
        <top>0</top>
    </coordinates>
    <controls>
        <control type="group">
            <!-- default group -->
            <control type="multiimage">
                <include>HomeBG</include>
                <imagepath background="true">$VAR[value_menutheme_settings]</imagepath>
            </control>
            <control type="image">
                <left>0</left>
                <top>0</top>
                <width>1920</width>
                <height>1080</height>
                <texture>darkgrey.png</texture>
                <colordiffuse>$VAR[value_texturecolor]</colordiffuse>
                <animation effect="fade" end="95" condition="true">Conditional</animation>
            </control>
            <control type="label" id="1">
                <left>30</left>
                <top>38</top>
                <width>1860</width>
                <height>74</height>
                <align>center</align>
                <font>Font_32</font>
                <textcolor>FFFFFAF0</textcolor>
            </control>
            <control type="image">
                <left>210</left>
                <top>120</top>
                <width>1500</width>
                <height>3</height>
                <align>left</align>
                <texture flipx="true">divider.png</texture>
            </control>
            <control type="image">
                <!-- Skin Logo -->
                <posx>1647</posx>
                <posy>32</posy>
                <width>210</width>
                <height>150</height>
                <texture>aeonmq6_logo.png</texture>
                <colordiffuse>FFFFFFFF</colordiffuse>
                <aspectratio align="center" aligny="top">keep</aspectratio>
            </control>
            <control type="label">
                <!-- Versao -->
                <left>1647</left>
                <top>135</top>
                <width>210</width>
                <height>38</height>
                <align>center</align>
                <font>Font_18</font>
                <textcolor>FFFFFAF0</textcolor>
                <label>$VAR[SkinVersion]</label>
            </control>
            <control type="label">
                <left>1647</left>
                <top>165</top>
                <width>210</width>
                <height>38</height>
                <align>center</align>
                <font>Font_18A</font>
                <textcolor>$VAR[value_headercolor]</textcolor>
                <label>by MarcosQui</label>
            </control>
            <control type="textbox" id="5">
                <left>210</left>
                <top>180</top>
                <width>1500</width>
                <height>795</height>
                <label>-</label>
                <font>Font_18A</font>
                <align>justify</align>
                <pagecontrol>61</pagecontrol>
            </control>
            <control type="scrollbar" id="61">
                <include>ScrollBar_Vertical</include>
                <left>75</left>
                <top>180</top>
                <height>795</height>
                <onleft>61</onleft>
                <onright>61</onright>
                <ondown>61</ondown>
                <onup>61</onup>
            </control>
        </control>
    </controls>
</window>
