<?xml version="1.0" encoding="UTF-8"?>
<window id="120">
    <!-- Aeon MQ 6 -->
    <defaultcontrol>101</defaultcontrol>
    <onunload>Skin.Reset(customosd)</onunload>
    <controls>
        <control type="group">
            <!-- Barra de info do modo custom -->
            <top>0</top>
            <include>Animation_TopPopup</include>
            <visible>Skin.HasSetting(customosd)</visible>
            <include>Dialogs_Bar</include>
            <control type="image">
                <!-- ativado azul -->
                <left>75</left>
                <top>8</top>
                <width>60</width>
                <height>28</height>
                <texture>grey.png</texture>
                <colordiffuse>FF007399</colordiffuse>
            </control>
            <control type="label">
                <left>150</left>
                <top>6</top>
                <width>375</width>
                <align>left</align>
                <font>Font_18A</font>
                <textcolor>FFFFFAF0</textcolor>
                <label>31426</label>
                <scroll>true</scroll>
                <scrollout>false</scrollout>
            </control>
            <control type="image">
                <!-- desativado vermelho -->
                <left>375</left>
                <top>8</top>
                <width>60</width>
                <height>28</height>
                <texture>grey.png</texture>
                <colordiffuse>FFB20000</colordiffuse>
            </control>
            <control type="label">
                <left>450</left>
                <top>6</top>
                <width>375</width>
                <align>left</align>
                <font>Font_18A</font>
                <textcolor>FFFFFAF0</textcolor>
                <label>31427</label>
                <scroll>true</scroll>
                <scrollout>false</scrollout>
            </control>
        </control>
        <control type="group">
            <top>780</top>
            <animation effect="slide" time="400" start="0,300" end="0,0" easing="out" tween="cubic">WindowOpen</animation>
            <animation effect="slide" time="400" start="0,300" end="0,0" easing="out" tween="cubic">Visible</animation>
            <animation effect="slide" time="400" start="0,0" end="0,300" easing="in" tween="cubic">WindowClose</animation>
            <animation effect="slide" time="400" start="0,0" end="0,300" easing="in" tween="cubic">Hidden</animation>
            <!-- Seek Widget -->
            <control type="group">
                <left>-750</left>
                <top>57</top>
                <width>748</width>
                <height>42</height>
                <animation effect="slide" end="300,0" time="200" condition="Player.Forwarding2x">Conditional</animation>
                <animation effect="slide" end="412,0" time="200" condition="Player.Forwarding4x">Conditional</animation>
                <animation effect="slide" end="525,0" time="200" condition="Player.Forwarding8x">Conditional</animation>
                <animation effect="slide" end="638,0" time="200" condition="Player.Forwarding16x">Conditional</animation>
                <animation effect="slide" end="748,0" time="200" condition="Player.Forwarding32x">Conditional</animation>
                <control type="image">
                    <left>0</left>
                    <top>0</top>
                    <width>748</width>
                    <height>42</height>
                    <texture flipy="true">osd_button_reverse.png</texture>
                </control>
                <control type="label">
                    <include>OSDSeekForward</include>
                    <label>$VAR[value_seekforward]</label>
                </control>
            </control>
            <control type="group">
                <left>1920</left>
                <top>57</top>
                <width>748</width>
                <height>42</height>
                <animation effect="slide" end="-300,0" time="200" condition="Player.Rewinding2x">Conditional</animation>
                <animation effect="slide" end="-412,0" time="200" condition="Player.Rewinding4x">Conditional</animation>
                <animation effect="slide" end="-525,0" time="200" condition="Player.Rewinding8x">Conditional</animation>
                <animation effect="slide" end="-638,0" time="200" condition="Player.Rewinding16x">Conditional</animation>
                <animation effect="slide" end="-748,0" time="200" condition="Player.Rewinding32x">Conditional</animation>
                <control type="image">
                    <left>0</left>
                    <top>0</top>
                    <width>748</width>
                    <height>42</height>
                    <texture flipy="true" flipx="true">osd_button_reverse.png</texture>
                </control>
                <control type="label">
                    <include>OSDSeekReverse</include>
                    <label>$VAR[value_seekreverse]</label>
                </control>
            </control>
            <control type="image">
                <!-- Barra de fundo -->
                <left>0</left>
                <top>54</top>
                <width>1920</width>
                <height>45</height>
                <texture>shadow_h.png</texture>
                <colordiffuse>C3FFFFFF</colordiffuse>
            </control>
            <control type="image">
                <left>0</left>
                <top>98</top>
                <height>202</height>
                <width>1920</width>
                <texture>darkgrey.png</texture>
                <colordiffuse>$VAR[value_texturecolor]</colordiffuse>
                <include>Animation_HomeGlass2</include>
            </control>
            <!-- Menu Content -->
            <include condition="!Skin.HasSetting(audioosdfixedlist)">AudioOSDWrapList</include>
            <include condition="Skin.HasSetting(audioosdfixedlist)">AudioOSDFixedList</include>
            <control type="button" id="150">
                <!-- Previous -->
                <include>InvisibleButton</include>
                <onfocus>PlayerControl(Previous)</onfocus>
                <onfocus>SetFocus(23)</onfocus>
            </control>
            <control type="button" id="151">
                <!-- Next -->
                <include>InvisibleButton</include>
                <onfocus>PlayerControl(Next)</onfocus>
                <onfocus>SetFocus(17)</onfocus>
            </control>
            <control type="button" id="152">
                <!-- VolumeUp -->
                <include>InvisibleButton</include>
                <onfocus>VolumeUp</onfocus>
                <onfocus>SetFocus(13)</onfocus>
            </control>
            <control type="button" id="153">
                <!-- VolumeDown -->
                <include>InvisibleButton</include>
                <onfocus>VolumeDown</onfocus>
                <onfocus>SetFocus(14)</onfocus>
            </control>
            <control type="button" id="154">
                <!-- VolumeMute -->
                <include>InvisibleButton</include>
                <onfocus>Mute</onfocus>
                <onfocus>SetFocus(15)</onfocus>
            </control>
            <control type="button" id="155">
                <!-- UserRating -->
                <include>InvisibleButton</include>
                <onfocus>UserRating</onfocus>
                <onfocus>SetFocus(28)</onfocus>
            </control>
            <control type="progress">
                <left>45</left>
                <top>110</top>
                <width>1830</width>
                <height>38</height>
                <info>Player.Progress</info>
            </control>
            <control type="slider" id="888">
                <left>58</left>
                <top>112</top>
                <width>1803</width>
                <height>32</height>
                <onup>Dialog.Close(120)</onup>
                <ondown>101</ondown>
                <action>seek</action>
                <texturesliderbar>sliderback_large.png</texturesliderbar>
                <textureslidernib>-</textureslidernib>
                <textureslidernibfocus>progress_mid.png</textureslidernibfocus>
                <animation effect="fade" time="200">VisibleChange</animation>
            </control>
            <control type="label">
                <left>135</left>
                <top>110</top>
                <width>1650</width>
                <height>45</height>
                <align>center</align>
                <font>Font_17</font>
                <textcolor>FFFFFAF0</textcolor>
                <shadowcolor>grey</shadowcolor>
                <label>[B]$VAR[value_osd_timeduration][/B]</label>
            </control>
            <control type="label">
                <!-- nome dos botoes -->
                <left>480</left>
                <top>258</top>
                <width>960</width>
                <height>38</height>
                <align>center</align>
                <font>Font_18</font>
                <label>$VAR[value_osdmusic_title]</label>
                <textcolor>FFFFFAF0</textcolor>
            </control>
            <control type="group">
                <include>Animation_Fade</include>
                <visible>!Skin.HasSetting(customosd)</visible>
                <control type="label">
                    <!-- hora atual -->
                    <left>45</left>
                    <top>195</top>
                    <width>450</width>
                    <height>38</height>
                    <font>Font_18A</font>
                    <textcolor>90FFFFFF</textcolor>
                    <label>31310</label>
                </control>
                <control type="label">
                    <left>45</left>
                    <top>232</top>
                    <width>450</width>
                    <height>38</height>
                    <font>Font_32</font>
                    <textcolor>FFFFFAF0</textcolor>
                    <label>$INFO[System.Time]</label>
                </control>
                <control type="label">
                    <!-- termino -->
                    <left>1425</left>
                    <top>195</top>
                    <width>450</width>
                    <height>38</height>
                    <align>right</align>
                    <font>Font_18A</font>
                    <textcolor>90FFFFFF</textcolor>
                    <label>31311</label>
                </control>
                <control type="label">
                    <left>1425</left>
                    <top>232</top>
                    <width>450</width>
                    <height>38</height>
                    <align>right</align>
                    <font>Font_32</font>
                    <textcolor>FFFFFAF0</textcolor>
                    <label>$INFO[Player.FinishTime]</label>
                </control>
            </control>
            <control type="image">
                <left>0</left>
                <top>172</top>
                <height>135</height>
                <width>1920</width>
                <texture>gradient_small.png</texture>
            </control>
            <control type="image">
                <left>0</left>
                <top>98</top>
                <height>202</height>
                <width>1920</width>
                <colordiffuse>99FFFFFF</colordiffuse>
                <texture>osd_glass.png</texture>
            </control>
        </control>
        <include>BGBlack</include>
    </controls>
</window>
