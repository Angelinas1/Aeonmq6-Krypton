<?xml version="1.0" encoding="UTF-8"?>
<window>
    <!-- <visible>Window.IsActive(fullscreenvideo) + !Window.IsActive(script.pseudotv.TVOverlay.xml) + !Window.IsActive(script.pseudotv.live.TVOverlay.xml)</visible> -->
    <!-- Aeon MQ 6 -->
    <defaultcontrol>23</defaultcontrol>
    <visible>Window.IsActive(fullscreenvideo) + ![Window.IsActive(videoosd) | Window.IsActive(fullscreeninfo) | Window.IsActive(osdvideosettings) | Window.IsActive(osdaudiosettings) | Window.IsActive(SubtitleSearch)] + [Player.Seeking | Player.DisplayAfterSeek | Player.Paused | Player.Forwarding | Player.Rewinding]</visible>
    <controls>
        <control type="group">
            <animation effect="slide" time="400" start="0,-375" end="0,0" easing="inout" tween="cubic">WindowOpen</animation>
            <animation effect="slide" time="400" start="0,-375" end="0,0" easing="inout" tween="cubic">Visible</animation>
            <animation effect="slide" time="400" start="0,0" end="0,-375" easing="inout" tween="cubic">WindowClose</animation>
            <animation effect="slide" time="400" start="0,0" end="0,-375" easing="inout" tween="cubic">Hidden</animation>
            <visible>![SubString(skin.string(videofurniture),hide) | Window.IsActive(pvrosdguide) | Window.IsActive(pvrosdchannels)]</visible>
            <control type="image">
                <left>0</left>
                <top>0</top>
                <height>225</height>
                <width>1920</width>
                <texture flipy="true" diffuse="shadow_mask.png">darkgrey.png</texture>
                <colordiffuse>$VAR[value_texturecolor]</colordiffuse>
                <animation effect="fade" end="85" condition="true">Conditional</animation>
            </control>
            <control type="image" id="82">
                <!-- Logo e Clearart -->
                <left>45</left>
                <top>12</top>
                <width>390</width>
                <height>225</height>
                <aspectratio aligny="top">keep</aspectratio>
                <texture background="true">$VAR[value_art_full]</texture>
                <visible>!StringCompare(skin.string(fullscreen_icon),noicon)</visible>
            </control>
            <control type="label">
                <!-- Title -->
                <left>45</left>
                <top>12</top>
                <width>1605</width>
                <height>60</height>
                <font>Font_40</font>
                <label>$VAR[value_videoosdtitle]</label>
                <textcolor>FFFFFAF0</textcolor>
                <scroll>true</scroll>
                <scrollout>false</scrollout>
                <visible>StringCompare(skin.string(fullscreen_icon),noicon) | VideoPlayer.Content(movies) + IsEmpty(Player.Art(clearlogo)) | VideoPlayer.Content(episodes) + IsEmpty(Player.Art(tvshow.clearlogo))</visible>
            </control>
            <control type="label">
                <left>45</left>
                <top>82</top>
                <width>1575</width>
                <height>38</height>
                <font>Font_18A</font>
                <label>$VAR[value_videoosdtag]</label>
                <textcolor>90FFFFFF</textcolor>
                <scroll>true</scroll>
                <scrollout>false</scrollout>
                <visible>StringCompare(skin.string(fullscreen_icon),noicon)</visible>
            </control>
            <control type="image">
                <!-- Star -->
                <left>1672</left>
                <top>22</top>
                <width>225</width>
                <height>38</height>
                <align>right</align>
                <aligny>center</aligny>
                <texture>$VAR[value_player_starrating]</texture>
                <colordiffuse>$VAR[value_headercolor]</colordiffuse>
                <aspectratio align="center" aligny="center">keep</aspectratio>
                <visible>!VideoPlayer.Content(LiveTV)</visible>
            </control>
            <control type="label">
                <left>1650</left>
                <top>45</top>
                <width>225</width>
                <height>90</height>
                <align>right</align>
                <aligny>center</aligny>
                <font>Font_18A</font>
                <label>$INFO[VideoPlayer.RatingAndVotes]</label>
                <textcolor>$VAR[value_headercolor]</textcolor>
                <scroll>true</scroll>
                <scrollout>false</scrollout>
                <visible>!VideoPlayer.Content(LiveTV)</visible>
            </control>
            <control type="group">
                <left>1760</left>
                <top>112</top>
                <visible>!SubString(VideoPlayer.top250,0,Left) + !IsEmpty(VideoPlayer.top250)</visible>
                <control type="image">
                    <!-- Top 250 -->
                    <include>FlagImage</include>
                    <texture>flags/imdb_250.png</texture>
                </control>
                <control type="label">
                    <!-- Top 250 info -->
                    <top>9</top>
                    <font>Font_20</font>
                    <include>FlagLabel</include>
                    <label>$INFO[VideoPlayer.Top250]</label>
                </control>
            </control>
        </control>
        <control type="group">
            <animation effect="slide" time="400" start="0,1200" end="0,0" easing="out" tween="cubic">WindowOpen</animation>
            <animation effect="slide" time="400" start="0,0" end="0,1200" easing="in" tween="cubic">WindowClose</animation>
            <visible>!SubString(skin.string(videofurniture),hide)</visible>
            <control type="image">
                <left>0</left>
                <top>825</top>
                <height>255</height>
                <width>1920</width>
                <texture diffuse="shadow_mask.png">darkgrey.png</texture>
                <colordiffuse>$VAR[value_floorcolor]</colordiffuse>
                <animation effect="fade" end="95" condition="true">Conditional</animation>
            </control>
            <control type="group">
                <animation effect="slide" start="0,0" end="0,-38" time="300" tween="cubic" easing="out" condition="Player.Forwarding">Conditional</animation>
                <control type="progress" id="23">
                    <left>30</left>
                    <top>996</top>
                    <width>375</width>
                    <height>15</height>
                    <info>Player.Progress</info>
                </control>
                <control type="label">
                    <left>45</left>
                    <top>1020</top>
                    <width>345</width>
                    <height>38</height>
                    <font>Font_23</font>
                    <align>center</align>
                    <textcolor>FFFFFAF0</textcolor>
                    <label>$INFO[VideoPlayer.Time] / $INFO[VideoPlayer.Duration]</label>
                    <visible>!Player.Seeking</visible>
                </control>
                <control type="label">
                    <left>45</left>
                    <top>1020</top>
                    <width>345</width>
                    <height>38</height>
                    <font>Font_23</font>
                    <align>center</align>
                    <textcolor>FFFFFAF0</textcolor>
                    <label>$INFO[Player.SeekTime] / $INFO[VideoPlayer.Duration]</label>
                    <visible>Player.Seeking</visible>
                </control>
                <control type="label">
                    <left>300</left>
                    <top>1020</top>
                    <width>345</width>
                    <height>38</height>
                    <font>Font_23</font>
                    <align>center</align>
                    <textcolor>FFFFFAF0</textcolor>
                    <label>$VAR[SeekLabel]</label>
                    <visible>Player.Seeking | Player.DisplayAfterSeek</visible>
                </control>
            </control>
            <control type="group">
                <animation effect="slide" start="0,0" end="0,-38" time="300" tween="cubic" easing="out" condition="Player.Rewinding">Conditional</animation>
                <control type="label">
                    <!-- Hora atual -->
                    <left>1320</left>
                    <top>996</top>
                    <width>180</width>
                    <height>38</height>
                    <align>right</align>
                    <font>Font_15</font>
                    <textcolor>$VAR[value_headercolor]</textcolor>
                    <label>31310</label>
                </control>
                <control type="label">
                    <left>1320</left>
                    <top>1020</top>
                    <width>180</width>
                    <height>38</height>
                    <align>right</align>
                    <font>Font_23</font>
                    <textcolor>FFFFFAF0</textcolor>
                    <label>$INFO[System.Time]</label>
                </control>
                <control type="image">
                    <left>1521</left>
                    <top>998</top>
                    <height>60</height>
                    <width>1</width>
                    <texture>line_v.png</texture>
                </control>
                <control type="label">
                    <!-- Tempo restante -->
                    <left>1508</left>
                    <top>996</top>
                    <width>180</width>
                    <height>38</height>
                    <align>right</align>
                    <font>Font_15</font>
                    <textcolor>$VAR[value_headercolor]</textcolor>
                    <label>31991</label>
                </control>
                <control type="label">
                    <left>1508</left>
                    <top>1020</top>
                    <width>180</width>
                    <height>38</height>
                    <align>right</align>
                    <font>Font_23</font>
                    <textcolor>FFFFFAF0</textcolor>
                    <label>$INFO[Player.TimeRemaining]</label>
                </control>
                <control type="image">
                    <left>1708</left>
                    <top>998</top>
                    <height>60</height>
                    <width>1</width>
                    <texture>line_v.png</texture>
                </control>
                <control type="label">
                    <!-- Hora do termino -->
                    <left>1695</left>
                    <top>996</top>
                    <width>180</width>
                    <height>38</height>
                    <align>right</align>
                    <font>Font_15</font>
                    <textcolor>$VAR[value_headercolor]</textcolor>
                    <label>31311</label>
                    <scroll>true</scroll>
                </control>
                <control type="label">
                    <left>1695</left>
                    <top>1020</top>
                    <width>180</width>
                    <height>38</height>
                    <align>right</align>
                    <font>Font_23</font>
                    <textcolor>FFFFFAF0</textcolor>
                    <label>$INFO[Player.FinishTime]</label>
                </control>
            </control>
        </control>
        <control type="group">
            <visible>Player.Paused</visible>
            <top>-52</top>
            <animation effect="slide" time="800" start="0,1200" end="0,0" easing="inout" tween="cubic" condition="System.IdleTime(3)">Conditional</animation>
            <animation effect="slide" time="800" start="0,0" end="0,1200" easing="inout" tween="cubic" condition="!System.IdleTime(3)">Conditional</animation>
            <animation effect="slide" start="0,0" end="0,750" time="700" tween="cubic" easing="out" condition="SubString(skin.string(videofurniture),partial) | SubString(skin.string(videofurniture),hide)">Conditional</animation>
            <control type="image">
                <!-- Barra de flags -->
                <left>330</left>
                <top>970</top>
                <height>46</height>
                <width>930</width>
                <texture>whitebar_h.png</texture>
                <colordiffuse>$VAR[value_floorcolor]</colordiffuse>
            </control>
            <control type="grouplist">
                <!-- Etiquetas -->
                <left>375</left>
                <top>956</top>
                <orientation>horizontal</orientation>
                <itemgap>20</itemgap>
                <control type="image">
                    <!-- Flags -->
                    <include>FlagImage</include>
                    <texture background="true">$VAR[value_mpaa_fs]</texture>
                </control>
                <control type="image">
                    <include>FlagImage</include>
                    <texture background="true">$VAR[value_videocodec_fs]</texture>
                </control>
                <control type="image">
                    <!-- Resolution -->
                    <include>FlagImage</include>
                    <texture>$INFO[VideoPlayer.VideoResolution,flags/resolution/,.png]</texture>
                </control>
                <control type="image">
                    <!-- Aspect -->
                    <include>FlagImage</include>
                    <texture>$INFO[Videoplayer.VideoAspect,flags/aspectratio/,.png]</texture>
                </control>
                <control type="image">
                    <!-- Audio Codec -->
                    <include>FlagImage</include>
                    <texture>$INFO[VideoPlayer.AudioCodec,flags/audiocodec/,.png]</texture>
                </control>
                <control type="image">
                    <!-- Channels -->
                    <include>FlagImage</include>
                    <texture>$INFO[VideoPlayer.AudioChannels,flags/audiochannels/,.png]</texture>
                </control>
            </control>
            <control type="image">
                <!-- Logo e Clearart -->
                <left>1620</left>
                <top>110</top>
                <width>270</width>
                <height>510</height>
                <aspectratio aligny="bottom">keep</aspectratio>
                <texture background="true">$VAR[value_art]</texture>
            </control>
            <control type="image">
                <!-- Fundo -->
                <left>0</left>
                <top>602</top>
                <width>1920</width>
                <height>450</height>
                <texture>widget_fullscreen.png</texture>
                <colordiffuse>$VAR[value_texturecolor]</colordiffuse>
                <include>Animation_DefaultGlass</include>
            </control>
            <control type="image">
                <!-- Barra de titulos -->
                <left>0</left>
                <top>621</top>
                <width>1920</width>
                <height>30</height>
                <texture>darkgrey.png</texture>
                <colordiffuse>$VAR[value_texturecolor]</colordiffuse>
                <include>Animation_DefaultGlass</include>
            </control>
            <control type="group">
                <!-- CDart -->
                <animation effect="slide" time="800" start="0,128" end="0,0" easing="inout" tween="cubic" condition="System.IdleTime(4)">Conditional</animation>
                <animation effect="slide" time="800" start="0,0" end="0,128" easing="inout" tween="cubic" condition="!System.IdleTime(4)">Conditional</animation>
                <visible>!IsEmpty(Player.Art(discart))</visible>
                <control type="image">
                    <animation effect="rotate" end="-360" center="auto" time="2000" loop="true" reversible="false" condition="StringCompare(skin.string(cdart_seconds),2000)">Conditional</animation>
                    <animation effect="rotate" end="-360" center="auto" time="4000" loop="true" reversible="false" condition="StringCompare(skin.string(cdart_seconds),4000)">Conditional</animation>
                    <animation effect="rotate" end="-360" center="auto" time="8000" loop="true" reversible="false" condition="StringCompare(skin.string(cdart_seconds),8000)">Conditional</animation>
                    <animation effect="rotate" end="-360" center="auto" time="16000" loop="true" reversible="false" condition="StringCompare(skin.string(cdart_seconds),16000)">Conditional</animation>
                    <animation effect="rotate" end="-360" center="auto" time="20000" loop="true" reversible="false" condition="StringCompare(skin.string(cdart_seconds),20000)">Conditional</animation>
                    <left>30</left>
                    <top>488</top>
                    <width>270</width>
                    <height>270</height>
                    <texture diffuse="cdart_diffuse.png" background="true">$VAR[value_videodisc]</texture>
                    <aspectratio align="center" aligny="center">keep</aspectratio>
                    <visible>!IsEmpty(Player.Art(discart))</visible>
                </control>
            </control>
            <control type="image">
                <left>12</left>
                <top>624</top>
                <width>291</width>
                <height>402</height>
                <texture>darkgrey.png</texture>
                <colordiffuse>$VAR[value_texturecolor]</colordiffuse>
            </control>
            <control type="image">
                <!-- Cover -->
                <left>15</left>
                <top>624</top>
                <width>285</width>
                <height>402</height>
                <texture>$VAR[value_thumbfullscreen]</texture>
                <aspectratio align="right">keep</aspectratio>
                <bordertexture border="-2">darkgrey.png</bordertexture>
            </control>
            <control type="label">
                <!-- Titulo enredo -->
                <left>330</left>
                <top>618</top>
                <width>450</width>
                <height>38</height>
                <align>left</align>
                <font>Font_17</font>
                <textcolor>$VAR[value_headercolor]</textcolor>
                <label>31530</label>
            </control>
            <control type="textbox">
                <!-- Plot -->
                <left>330</left>
                <top>668</top>
                <width>930</width>
                <height>270</height>
                <font>Font_18A</font>
                <label>$INFO[VideoPlayer.Plot]</label>
                <textcolor>FFFFFAF0</textcolor>
                <autoscroll delay="20000" time="4000" repeat="15000">true</autoscroll>
            </control>
            <control type="group">
                <visible>!VideoPlayer.Content(LiveTV)</visible>
                <control type="label">
                    <!-- Titulo elenco -->
                    <left>1320</left>
                    <top>618</top>
                    <width>450</width>
                    <height>38</height>
                    <align>left</align>
                    <font>Font_17</font>
                    <textcolor>$VAR[value_headercolor]</textcolor>
                    <label>31703</label>
                </control>
                <control type="textbox">
                    <!-- Cast -->
                    <left>1320</left>
                    <top>668</top>
                    <width>570</width>
                    <height>345</height>
                    <font>Font_18A</font>
                    <label>$INFO[VideoPlayer.CastAndRole]</label>
                    <textcolor>FFFFFAF0</textcolor>
                    <autoscroll delay="20000" time="4000" repeat="15000">true</autoscroll>
                    <visible>Player.Paused</visible>
                </control>
            </control>
        </control>
        <!-- Seek Widget -->
        <control type="group">
            <animation type="WindowOpen" condition="true">
                <effect type="slide" time="400" start="0,1200" end="0,0" easing="out" tween="cubic" />
            </animation>
            <animation type="WindowClose" condition="true">
                <effect type="slide" time="400" start="0,0" end="0,1200" easing="in" tween="cubic" />
            </animation>
            <control type="group">
                <left>-750</left>
                <top>1040</top>
                <width>748</width>
                <height>42</height>
                <animation effect="slide" end="300,0" time="200" condition="Player.Forwarding2x">Conditional</animation>
                <animation effect="slide" end="412,0" time="200" condition="Player.Forwarding4x">Conditional</animation>
                <animation effect="slide" end="525,0" time="200" condition="Player.Forwarding8x">Conditional</animation>
                <animation effect="slide" end="638,0" time="200" condition="Player.Forwarding16x">Conditional</animation>
                <animation effect="slide" end="748,0" time="200" condition="Player.Forwarding32x">Conditional</animation>
                <control type="image">
                    <left>0</left>
                    <top>0</top>
                    <width>748</width>
                    <height>42</height>
                    <texture flipy="true">osd_button_reverse.png</texture>
                </control>
                <control type="label">
                    <include>OSDSeekForward</include>
                    <label>$VAR[value_seekforward]</label>
                </control>
            </control>
            <control type="group">
                <left>1920</left>
                <top>1040</top>
                <width>748</width>
                <height>42</height>
                <animation effect="slide" end="-300,0" time="200" condition="Player.Rewinding2x">Conditional</animation>
                <animation effect="slide" end="-412,0" time="200" condition="Player.Rewinding4x">Conditional</animation>
                <animation effect="slide" end="-525,0" time="200" condition="Player.Rewinding8x">Conditional</animation>
                <animation effect="slide" end="-638,0" time="200" condition="Player.Rewinding16x">Conditional</animation>
                <animation effect="slide" end="-748,0" time="200" condition="Player.Rewinding32x">Conditional</animation>
                <control type="image">
                    <left>0</left>
                    <top>0</top>
                    <width>748</width>
                    <height>42</height>
                    <texture flipy="true" flipx="true">osd_button_reverse.png</texture>
                </control>
                <control type="label">
                    <include>OSDSeekReverse</include>
                    <label>$VAR[value_seekreverse]</label>
                </control>
            </control>
        </control>
        <control type="label">
            <left>-480</left>
            <top>270</top>
            <width>2880</width>
            <height>158</height>
            <align>center</align>
            <aligny>center</aligny>
            <font>Font_100</font>
            <label>31737</label>
            <textcolor>80FFFAF0</textcolor>
            <visible>Player.Paused + !Skin.HasSetting(nopausedlabel)</visible>
            <animation type="Visible">
                <effect type="slide" start="0,-900" end="0,0" time="600" tween="cubic" easing="out" />
                <effect type="fade" start="0" end="100" time="600" />
            </animation>
            <animation type="WindowOpen">
                <effect type="slide" start="0,-900" end="0,0" time="600" tween="cubic" easing="out" />
                <effect type="fade" start="0" end="100" time="600" />
            </animation>
            <animation type="WindowClose">
                <effect type="slide" start="0,0" end="-900,0" time="600" tween="cubic" easing="out" />
                <effect type="fade" start="100" end="0" time="600" />
            </animation>
            <animation type="Hidden">
                <effect type="slide" start="0,0" end="-900,0" time="600" tween="cubic" easing="out" />
                <effect type="fade" start="100" end="0" time="600" />
            </animation>
        </control>
    </controls>
</window>
