                </control>
            </control>
            <control type="image">
                <left>0</left>
                <top>172</top>
                <height>135</height>
                <width>1920</width>
                <texture>gradient_small.png</texture>
            </control>
            <control type="image">
                <left>0</left>
                <top>98</top>
                <height>195</height>
                <width>1920</width>
                <colordiffuse>99FFFFFF</colordiffuse>
                <texture>osd_glass.png</texture>
            </control>
        </control>
        <include>BGBlack</include>
    </controls>
</window>
