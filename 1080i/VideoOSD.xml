                    <top>195</top>
                    <width>450</width>
                    <height>38</height>
                    <align>right</align>
                    <font>Font_18A</font>
                    <textcolor>90FFFFFF</textcolor>
                    <label>31311</label>
                    <visible>!VideoPlayer.Content(LiveTV)</visible>
                </control>
                <control type="label">
                    <left>1425</left>
                    <top>232</top>
                    <width>450</width>
                    <height>38</height>
                    <align>right</align>
                    <font>Font_32</font>
                    <textcolor>FFFFFAF0</textcolor>
                    <label>$INFO[Player.FinishTime]</label>
