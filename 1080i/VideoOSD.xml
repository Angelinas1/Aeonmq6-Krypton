                <colordiffuse>FF007399</colordiffuse>
            </control>
            <control type="label">
                <left>150</left>
                <top>6</top>
                <width>375</width>
                <align>left</align>
                <font>Font_18A</font>
                <textcolor>FFFFFAF0</textcolor>
                <label>31426</label>
                <scroll>true</scroll>
                <scrollout>false</scrollout>
            </control>
            <control type="image">
                <!-- desativado vermelho -->
                <left>375</left>
                <top>8</top>
