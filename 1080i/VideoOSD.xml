                <control type="label">
                    <!-- hora atual -->
                    <left>45</left>
                    <top>195</top>
                    <width>450</width>
                    <height>38</height>
                    <font>Font_18A</font>
                    <textcolor>90FFFFFF</textcolor>
                    <label>31310</label>
