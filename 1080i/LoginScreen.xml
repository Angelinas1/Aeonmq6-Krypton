<?xml version="1.0" encoding="UTF-8"?>
<window type="dialog" id="29">
    <!-- Aeon MQ 6 -->
    <defaultcontrol always="true">52</defaultcontrol>
    <allowoverlay>no</allowoverlay>
    <coordinates>
        <system>1</system>
        <left>0</left>
        <top>0</top>
    </coordinates>
    <controls>
        <control type="group">
            <!-- default group -->
            <include>Animation_Window</include>
            <control type="image">
                <left>0</left>
                <top>0</top>
                <width>1920</width>
                <height>1080</height>
                <texture background="true">special://skin/backgrounds/vnodes.jpg</texture>
                <include>Animation_Fade</include>
            </control>
            <control type="image">
                <!-- Barra do rodape -->
                <left>0</left>
                <top>660</top>
                <width>1920</width>
                <height>420</height>
                <colordiffuse>FFFFFAF0</colordiffuse>
                <texture>shadow_mask.png</texture>
                <include>Animation_FloorGlass</include>
            </control>
            <control type="image">
                <left>0</left>
                <top>660</top>
                <width>1920</width>
                <height>420</height>
                <colordiffuse>$VAR[value_texturecolor]</colordiffuse>
                <texture diffuse="shadow_mask.png">shadow_spot.png</texture>
                <animation effect="fade" end="75" condition="true">Conditional</animation>
            </control>
            <control type="fixedlist" id="52">
                <viewtype label="login">list</viewtype>
                <left>-68</left>
                <top>622</top>
                <width>2169</width>
                <height>825</height>
                <orientation>horizontal</orientation>
                <focusposition>3</focusposition>
                <scrolltime>400</scrolltime>
                <onleft>90</onleft>
                <onright>90</onright>
                <onup>52</onup>
                <ondown>98</ondown>
                <itemlayout height="300" width="300">
                    <control type="image">
                        <left>3</left>
                        <top>351</top>
                        <width>225</width>
                        <height>225</height>
                        <fadetime>FanartCrossFadeTime</fadetime>
                        <aspectratio align="center" aligny="top">keep</aspectratio>
                        <texture flipy="true" diffuse="thumb_box_reflect.png" fallback="DefaultUser.png" background="true">$INFO[ListItem.Icon]</texture>
                        <bordertexture flipy="true" diffuse="thumb_box_reflect.png" border="-2">black.png</bordertexture>
                    </control>
                    <control type="image">
                        <left>3</left>
                        <top>120</top>
                        <width>225</width>
                        <height>225</height>
                        <fadetime>FanartCrossFadeTime</fadetime>
                        <aspectratio align="center" aligny="bottom">keep</aspectratio>
                        <texture fallback="DefaultUser.png" background="true">$INFO[ListItem.Icon]</texture>
                        <bordertexture border="-2">black.png</bordertexture>
                    </control>
                </itemlayout>
                <focusedlayout height="300" width="300">
                    <control type="image">
                        <left>3</left>
                        <top>357</top>
                        <width>225</width>
                        <height>225</height>
                        <fadetime>FanartCrossFadeTime</fadetime>
                        <aspectratio align="center" aligny="top">keep</aspectratio>
                        <texture flipy="true" diffuse="thumb_box_reflect.png" fallback="DefaultUser.png" background="true">$INFO[ListItem.Icon]</texture>
                        <bordertexture flipy="true" diffuse="thumb_box_reflect.png" border="-1">grey.png</bordertexture>
                        <animation effect="zoom" start="100" end="145" center="112,368" time="400" easing="out" reversible="false" tween="cubic">Focus</animation>
                    </control>
                    <control type="image">
                        <left>3</left>
                        <top>128</top>
                        <width>225</width>
                        <height>225</height>
                        <fadetime>FanartCrossFadeTime</fadetime>
                        <aspectratio align="center" aligny="bottom">keep</aspectratio>
                        <texture fallback="DefaultUser.png" background="true">$INFO[ListItem.Icon]</texture>
                        <bordertexture border="-1">grey.png</bordertexture>
                        <animation effect="zoom" start="100" end="145" center="112,368" time="400" easing="out" reversible="false" tween="cubic">Focus</animation>
                    </control>
                    <control type="image">
                        <left>285</left>
                        <top>17</top>
                        <width>750</width>
                        <height>75</height>
                        <texture>gradient_button.png</texture>
                        <colordiffuse>95000000</colordiffuse>
                        <visible>Control.HasFocus(52)</visible>
                    </control>
                    <control type="label">
                        <left>300</left>
                        <top>9</top>
                        <width>900</width>
                        <height>24</height>
                        <align>left</align>
                        <font>Font_30</font>
                        <textcolor>$VAR[value_headercolor]</textcolor>
                        <info>ListItem.Label</info>
                        <scroll>true</scroll>
                        <visible>Control.HasFocus(52)</visible>
                    </control>
                    <control type="label">
                        <left>300</left>
                        <top>60</top>
                        <width>900</width>
                        <height>24</height>
                        <align>left</align>
                        <font>Font_15</font>
                        <textcolor>FFFFFAF0</textcolor>
                        <info>ListItem.Label2</info>
                        <scroll>true</scroll>
                        <visible>Control.HasFocus(52)</visible>
                    </control>
                </focusedlayout>
            </control>
            <include>MultiData</include>
        </control>
    </controls>
</window>
